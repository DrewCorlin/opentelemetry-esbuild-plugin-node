{
  "name": "opentelemetry-esbuild-plugin-node",
  "version": "3.0.0",
  "description": "Metapackage which bundles opentelemetry node core and contrib instrumentations via an esbuild plugin",
  "author": "Drew Corlin",
  "homepage": "https://github.com/DrewCorlin/opentelemetry-esbuild-plugin-node",
  "repository": {
    "type": "git",
    "url": "https://github.com/DrewCorlin/opentelemetry-esbuild-plugin-node.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "exports": {
    ".": "./build/src/index.js"
  },
  "scripts": {
    "clean": "rimraf build/*",
    "compile": "tsc -p .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepublishOnly": "npm run compile",
    "tdd": "yarn test -- --watch-extensions ts --watch",
    "test": "nyc ts-mocha -p tsconfig.json 'test/**/*.ts'",
    "watch": "tsc -w"
  },
  "bugs": {
    "url": "https://github.com/DrewCorlin/opentelemetry-esbuild-plugin-node/issues"
  },
  "devDependencies": {
    "@fastify/rate-limit": "^10.2.2",
    "@opentelemetry/api": "^1.9.0",
    "@types/mocha": "7.0.2",
    "@types/node": "^20.17.17",
    "eslint": "^8.57.1",
    "@opentelemetry/sdk-node": "^0.200.0",
    "@types/semver": "^7.7.0",
    "@typescript-eslint/eslint-plugin": "8.28.0",
    "@typescript-eslint/parser": "8.28.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.2.5",
    "fastify": "5.2.2",
    "graphql": "^16.10.0",
    "mocha": "7.2.0",
    "mongodb": "6.8.0",
    "mongodb-memory-server": "9.5.0",
    "nyc": "17.1.0",
    "pino": "^9.6.0",
    "prettier": "3.5.3",
    "redis": "^4.7.0",
    "release-it": "^18.1.2",
    "rimraf": "6.0.1",
    "ts-mocha": "10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "@opentelemetry/auto-instrumentations-node": "^0.57.0",
    "@opentelemetry/instrumentation": "^0.200.0",
    "semver": "^7.7.1"
  },
  "peerDependencies": {
    "esbuild": ">=0.21.x <=0.25.x"
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.js.map",
    "build/src/**/*.d.ts"
  ]
}
