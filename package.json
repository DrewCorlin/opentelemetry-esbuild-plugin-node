{
  "name": "opentelemetry-esbuild-plugin-node",
  "version": "0.2.6-alpha.6",
  "description": "Metapackage which bundles opentelemetry node core and contrib instrumentations via an esbuild plugin",
  "author": "Drew Corlin",
  "homepage": "https://github.com/DrewCorlin/opentelemetry-esbuild-plugin-node",
  "repository": {
    "type": "git",
    "url": "https://github.com/DrewCorlin/opentelemetry-esbuild-plugin-node.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "exports": {
    ".": "./build/src/index.js"
  },
  "scripts": {
    "clean": "rimraf build/*",
    "compile": "tsc -p .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepublishOnly": "npm run compile",
    "tdd": "yarn test -- --watch-extensions ts --watch",
    "test": "nyc ts-mocha -p tsconfig.json 'test/**/*.ts'",
    "watch": "tsc -w"
  },
  "bugs": {
    "url": "https://github.com/DrewCorlin/opentelemetry-esbuild-plugin-node/issues"
  },
  "devDependencies": {
    "@fastify/rate-limit": "^9.1.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.55.0",
    "@types/mocha": "7.0.2",
    "@types/node": "18.6.5",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "eslint": "8.7.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "fastify": "4.15.0",
    "graphql": "^16.10.0",
    "mocha": "7.2.0",
    "mongodb": "6.8.0",
    "mongodb-memory-server": "9.5.0",
    "nyc": "15.1.0",
    "pino": "^8.19.0",
    "prettier": "2.8.8",
    "redis": "^4.7.0",
    "release-it": "^17.11.0",
    "rimraf": "5.0.5",
    "ts-mocha": "10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "@opentelemetry/auto-instrumentations-node": "0.53.0",
    "@opentelemetry/instrumentation": "^0.55.0",
    "esbuild": "0.24.x",
    "semver": "^7.6.3"
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.js.map",
    "build/src/**/*.d.ts"
  ]
}
